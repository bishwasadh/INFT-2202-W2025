{

    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "rootDir": "./src",
        "outDir": "./dist",
        "sourceMap": true,
        "moduleResolution": "node"
    },
    "include": ["src/**/*", "types/app.ts"],
}